/**
 * ビルド情報
 * ※このファイルはビルド時に自動生成されます
 */
export interface BuildInfo {
  /** セマンティックバージョン (e.g. 1.0.0) */
  version: string;
  /** ビルド番号 */
  buildNumber: number;
  /** フルバージョン (e.g. 1.0.0.123) */
  fullVersion: string;
  /** ビルド日時 (ISO形式) */
  buildDate: string;
}

/**
 * ビルド情報
